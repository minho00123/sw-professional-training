package com.my.sts05.controller;

import org.springframework.stereotype.Controller;
import org.springframework.ui.Model;
import org.springframework.web.bind.annotation.RequestMapping;

import com.my.sts05.model.DeptVo;

@Controller
public class Ex05Controller {

	@RequestMapping("/ex41.do")
	public void ex41() {
	}

	@RequestMapping("/ex42.do")
	public void ex42(int deptno, String dname, String loc, Model model) {
		model.addAttribute("bean", new DeptVo(deptno, dname, loc));
		model.addAttribute("dname", dname);
		model.addAttribute("loc", loc);
	}
}
