  	<servlet-name>detail</servlet-name>
  	<url-pattern>/detail</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>reply</servlet-name>
  	<servlet-class>com.mvc.controller.ReplyController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>reply</servlet-name>
  	<url-pattern>/reply</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>modify</servlet-name>
  	<servlet-class>com.mvc.controller.ModifyController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>modify</servlet-name>
  	<url-pattern>/modify</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>delete</servlet-name>
  	<servlet-class>com.mvc.controller.DeleteController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>delete</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
</web-app>