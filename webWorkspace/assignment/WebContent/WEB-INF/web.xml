<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>assignment</display-name>
  <welcome-file-list>
    <welcome-file>list.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>list</servlet-name>
  	<servlet-class>com.mvc.controller.ListController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>list</servlet-name>
  	<url-pattern>/list</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>write</servlet-name>
  	<servlet-class>com.mvc.controller.WriteController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>write</servlet-name>
  	<url-pattern>/write</url-pattern>
  </servlet-mapping>
  
  <servlet>
