<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>web17</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>jsonList</servlet-name>
		<servlet-class>com.web17.json.BbsListJson</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>jsonOne</servlet-name>
		<servlet-class>com.web17.json.BbsOneJson</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>jsonOne</servlet-name>
		<url-pattern>/bbs.json</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>jsonList</servlet-name>
		<url-pattern>/list.json</url-pattern>
	</servlet-mapping>
</web-app>