import org.eclipse.jetty.server.handler.ContextHandler;

public class App {

	public static void main(String[] args) {
		Server server = new Server(8080);
		server.setHandler(handler);
		try {
			server.start();
		} catch (Exception e) {
			System.out.println("서버구동 실패");
