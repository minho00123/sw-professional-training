S	ervletOutputStream out = res.getOutputStream();
	out.println("<!DOCTYPE html>");
	out.println("<html>");
	out.println("<head>");
	out.println("<meta charset=\"utf-8\">");
	out.println("</head>");
	out.println("<body>");
	out.write("<h1>한글로 작성</h1>".getBytes("utf-8"));
	out.println("</body>");
	out.println("</html>");
	out.close();
