package com.am;

public class Ex07 {

	public static void main(String[] args) {
		int me=1;
		int com=1;
		
		if(me==com) {
			System.out.println("╣Ф");
		}
		if(me==1 && com==2) {System.out.println("кл");}
		if(me==1 && com==3) {System.out.println("й┬");}

		if(me==2 && com==1) {System.out.println("й┬");}
		if(me==2 && com==3) {System.out.println("кл");}
		
		if(me==3 && com==1) {System.out.println("кл");}
		if(me==3 && com==2) {System.out.println("й┬");}
		
	}

}
