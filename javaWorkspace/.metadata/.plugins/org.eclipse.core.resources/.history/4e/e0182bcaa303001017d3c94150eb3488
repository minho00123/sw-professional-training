package com.am;

class Lec05{
	
	static Local func01() {
		class Local{
			void func02() {
				System.out.println("내부클래스의 기능수행");
			}
		}
//		local.func02();
		return new Local();
	}
}

public class Ex05 {

	public static void main(String[] args) {
		// 익명클래스 <- 엄청 많이 사용됨
		Local local=Lec05.func01();
		local.func02();

	}

}
