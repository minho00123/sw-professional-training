package com.am;

public class Ex11 {

	public static void main(String[] args) {
		char target='1';
		// 문자냐
		// 1.0.2
		System.out.println(Character.isDefined(target));
		// 1.5 문자냐
		System.out.println(Character.isLetter(target));
		// 1.7 문자냐
		System.out.println(Character.isAlphabetic(target));
	}

}
