package com.am;

class Carriage {
	int speed;
	int max;
	public Carriage() {
		max=40;
	}
	public void speedUp() {}
	public void speedDown() {}
}

class Car extends Carriage {
	String model;
	String color;
	public Car(String color, String model) {
		max=100;
	}
}

