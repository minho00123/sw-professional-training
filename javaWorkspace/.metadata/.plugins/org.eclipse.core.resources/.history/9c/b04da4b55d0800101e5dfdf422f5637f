package com.am;

import java.awt.Button;
import java.awt.Frame;

public class Ex06 extends Frame {

	public Ex06() {
		setLayout(null);
		
		Button btn1 = new Button("btn1");
		
		add(btn1);
		
		setLocation(100, 100);
		setSize(400, 300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new Ex06();
	}

}
