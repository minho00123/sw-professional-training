package com.pm;

public class Ex03 {

	public static void main(String[] args) {
		// CRUD
		String msg="ABCDabcd1234";
		
		System.out.println(msg.length());
		System.out.println(msg.charAt(4));
		msg=msg.replace("abcd", "aBCd");
		System.out.println(msg);
		msg=msg.replace("aBCd", "ad");
		System.out.println(msg);
	}

}
