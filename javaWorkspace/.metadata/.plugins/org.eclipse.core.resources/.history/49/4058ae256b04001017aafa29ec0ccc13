package com.am;

class MyArray {
	int[] arr=new int[0];
	
	void add(int su) {
		int[] before=arr;
		arr=new int[arr.length+1];
		for(int i=0; i<before.length; i++) {
			arr[i]=before[i];
		}
		arr[arr.length-1]=su;
	}
	int size() {
		return arr.length;
	}
	void get() {}
//	void remove() {}
//	void edit() {}
}

public class Ex03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
