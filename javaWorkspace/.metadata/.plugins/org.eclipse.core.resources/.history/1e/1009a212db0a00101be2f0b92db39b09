package com.pm;

import java.io.File;
import java.io.FileWriter;
import java.io.IOException;

public class Ex06 {

	public static void main(String[] args) {
		File file = new File("data06.txt");
		
		java.io.Writer fw = null;
		try {
			file.createNewFile();
			fw = new FileWriter(file);

			fw.write('วั');
			
			fw.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
