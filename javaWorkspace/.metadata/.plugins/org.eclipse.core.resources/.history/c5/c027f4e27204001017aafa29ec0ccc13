package com.am;

class Node {
	int val;
	Node nxt;
}

class MyLink {
	Node first;
	
	void add(int su) {
//		1.
		first=new Node();
//		temp.val=su;
		
//		2.
		Node temp2=new Node();
		temp2.val=su;
		first.nxt=temp2;
		
//		3.
		Node temp3=new Node();
		temp3.val=su;
		first.nxt.nxt=temp3;
	}
}

public class Ex05 {

	public static void main(String[] args) {

	}

}
