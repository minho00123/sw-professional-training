package com.am;

import java.util.ArrayList;
import java.util.List;

public class Ex06 {

	public static void main(String[] args) {
		List list = new ArrayList();
		list.add("첫번째");
		list.add("두번째");
		list.add("세번째");
		list.add("네번째");
		list.add("다섯번째");
		
		for (int i = 0; i < list.size(); i++) {
			System.out.println(list.get(i));
		}
	}

}
