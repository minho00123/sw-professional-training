package com.pm;

import java.awt.Frame;
import java.awt.Label;
import java.awt.Panel;

public class Ex02 {

	public static void main(String[] args) {
		Frame f = new Frame();
		Panel p = new Panel();

		Label la = new Label("abc");
		
		p.add(la);

		f.add(p);
		f.setLocation(100, 100);
		f.setSize(300, 300);
		f.setVisible(true);
	}

}
