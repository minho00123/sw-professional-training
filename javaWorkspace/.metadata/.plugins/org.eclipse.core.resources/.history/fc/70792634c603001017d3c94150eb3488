package com.pm;

public class Ex06 {

	public static void main(String[] args) {
		// 로또 번호 생성기(ver 0.1.0)
		
		System.out.println("로또 번호 생성기(ver 0.1.0)");
		
		int[] winningNumber = new int[6];
		int count = 0;
		for (int i = 0; i < 6; i++) {			
			for (int j = 0; j < winningNumber.length; i++) {
				int number = (int) (Math.random() * 45) + 1;

				if (number == winningNumber[j]) {
					continue;
				}
				
				
			}
		}
		

	}

}
