package com.am;

class Machine{
	public void on() {System.out.println("켜다");}
	public void off() {System.out.println("끄다");}
	public void work() {System.out.println("일하다");}
}
class Tv extends Machine{
	@Override
	public void work() {
		System.out.println("화면을 보여주다");
	}
}
class Radio{}

