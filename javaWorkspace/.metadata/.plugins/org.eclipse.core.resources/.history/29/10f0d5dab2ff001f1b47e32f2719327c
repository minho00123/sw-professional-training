package com.am;

class Ex77 {
	public void func01() {
		System.out.println("부모의 기능");
	}
}

public class Ex07 extends Ex77 {

	public static void main(String[] args) {
		// 상속
		// 모든 클래스는 부모클래스를 상속받음
		// 특별히 상속이 없으면 최상위의 Object 클래스를 상속
		// 단일 상속
		
		Ex77 parent = new Ex77();
		Ex07 me = new Ex07();
		parent.func01();
	}

}
