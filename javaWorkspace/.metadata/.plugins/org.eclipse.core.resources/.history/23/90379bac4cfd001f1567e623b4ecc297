/*
 * Q1. 구구단 출력(포멧을 그림처럼)
 * 
 * Q2.
 * A
 * BC
 * DEF
 * GHIJ
 * 
 * Q3. 가위 바위 보 게임(ver 0.1.0)
 */


public class Review {

	public static void main(String[] args) {
		System.out.println("Q1.");
		for (int i = 1; i <= 9; i++) {
			for (int j = 2; j <= 9; j++) {
				System.out.print(j+"x"+i+"= ");
			}
			System.out.println();
		}
		
		System.out.println("---------------------------------------------");
		System.out.println("Q2.");
		char c = 'A';
		for (char i = 0; i <= 4; i++) {
			for (int j = 0; j < i + 1; j++) {
				System.out.print((char) (c++));
			}
			System.out.println();
		}
	}

}
