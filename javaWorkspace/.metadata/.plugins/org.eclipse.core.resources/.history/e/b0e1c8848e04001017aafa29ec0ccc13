package com.pm;

import java.util.LinkedList;

public class Ex04 {

	public static void main(String[] args) {
		LinkedList list1 = new LinkedList();
		list1.add(1111);
		list1.add(2222);
		list1.add(3333);
		list1.add(4444);
		LinkedList list2 = new LinkedList(list1);

		for (int i = 0; i < list2.size(); i++) {
			System.out.println(list2.get(i));
		}
	}

}
