package com.pm;

import java.util.ArrayList;

public class Test {

	public static void main(String[] args) {
		ArrayList arr = new ArrayList();
		
		
		for (int i = 0; i < 5; i++) {
			int[] intArr = new int[3];
			arr.add(intArr);
		}
		
		for (int i = 0; i < 5; i++) {
			System.out.println(arr.get(i));
		}
				
		
		

	}

}
