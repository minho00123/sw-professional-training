package com.pm;

import java.util.ArrayList;

public class Ex02 {

	public static void main(String[] args) {
		ArrayList list = new ArrayList();
		list.add("첫번째");
		list.add("두번째");
		list.add("세번째");
		list.add("네번째");
		list.set(2, "3번째");
		list.remove(3);
		for (int i = 0; i < list.size(); i++) {
			Object ele = list.get(i);
			System.out.println((String) ele);
		}

	}

}
