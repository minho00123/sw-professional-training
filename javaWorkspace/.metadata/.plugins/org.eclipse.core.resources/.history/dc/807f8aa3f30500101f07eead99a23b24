package com.am;

import java.util.Arrays;

public class Ex02 {

	public static void main(String[] args) {
		int[] lotto = { 2, 44, 32, 12, 38, 8 };
		String[] arr2 = { "b", "Ab", "Aa", "B" };
		Arrays.sort(lotto);
		System.out.println(Arrays.toString(lotto));
		Arrays.sort(arr2);
		System.out.println(Arrays.toString(arr2));

	}

}
