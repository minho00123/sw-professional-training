package com.pm;

public class Ex07 {

	public static Number div(int a, int b) {
		if (a % b == 0) {
			return a / b;
		}
		return 1.0 * a / b;
	}

	public static Box<?> func1() {
		return new Box();
	}

	public static void main(String[] args) {
		Number result = div(5, 2);
		System.out.println(result);
	}

}
