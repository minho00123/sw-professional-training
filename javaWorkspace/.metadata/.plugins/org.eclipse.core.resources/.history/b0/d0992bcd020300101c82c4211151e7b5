package com.pm;
// 주민번호체크
// 학생성적
public class Ex17 {

	public static void main(String[] args) {
		System.out.println("학생성적관리프로그램(ver 0.8.0)");
		java.util.Scanner sc;
		sc=new java.util.Scanner(System.in);
		String input=null;
		while(true) {
			System.out.print("1.보기 2.입력 0.종료> ");
			input=sc.nextLine();
		}

	}

}
