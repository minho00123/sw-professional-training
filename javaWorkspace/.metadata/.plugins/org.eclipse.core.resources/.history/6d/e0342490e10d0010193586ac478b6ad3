package com.am;

import java.net.MalformedURLException;
import java.net.URL;

public class Ex03 {

	public static void main(String[] args) {
		String spec = "https://google.com";
		
		URL url = null;
		
		try {
			url = new URL(spec);
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}
	}

}
