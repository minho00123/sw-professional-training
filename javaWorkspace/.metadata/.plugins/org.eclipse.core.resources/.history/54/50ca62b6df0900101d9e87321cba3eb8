package com.am;

import java.io.File;

public class Ex01 {

	public static void main(String[] args) {
		// Input & Output
		java.io.File file;
//		file = new File("target.txt");
		file = new File("dir01");
		// 파일 - 디렉토리 & 파일

		System.out.println("존재하는가?" + file.exists());
		System.out.println("디렉토리인가?" + file.isDirectory());
		System.out.println("파일인가?" + file.isFile());

	}

}
