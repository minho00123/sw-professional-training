package com.pm;

public class Ex02 {
	// 메서드(클래스메서드, static method)
	public static void func01() {}

	// 메서드(멤버 메서드,인스턴스 메서드, non-static method)
	public void func02() {
		System.out.println("멤버메서드");
	}

	public static void main(String[] args) {
		func01();
		// 참조변수
		Ex02 me = new Ex02();
		me.func02();
	}

}
