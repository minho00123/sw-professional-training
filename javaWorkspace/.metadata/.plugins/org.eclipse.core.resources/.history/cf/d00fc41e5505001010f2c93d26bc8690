package com.pm;

class Lec05<T> {

	public T pass(T t) {
		return t;
	}
}

public class Ex05 {

	public static void main(String[] args) {
		Lec05<String> you = new Lec05();
//		Lec05 you = new Lec05<String>();
		String msg = you.pass("aaaaaa");
		String msg = you.pass(1234);
		System.out.println(msg);
	}

}
