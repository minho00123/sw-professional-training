package com.am;

import java.util.Iterator;
import java.util.Set;
import java.util.TreeSet;

class Ball implements Comparable {
	int num;

	public Ball(int num) {
		this.num = num;
	}

	@Override
	public int compareTo(Object obj) {
//		return 0;
//		return 1;
//		return -1;
		return this.num - ((Ball) obj).num;
	}

	@Override
	public String toString() {
		return "num=" + this.num;
	}
}

public class Ex02 {

	public static void main(String[] args) {
		// HashSet
		// set - 중복불허, 순서있음
		Set set1 = null;
//		set1 = new HashSet();
		set1 = new TreeSet();

		set1.add(new Ball(1));
		set1.add(new Ball(3));
		set1.add(new Ball(5));
		set1.add(new Ball(6));
		set1.add(new Ball(4));
		set1.add(new Ball(2));

		Iterator ite = set1.iterator();
		while (ite.hasNext()) {
			System.out.println(ite.next());
		}

	}

}
