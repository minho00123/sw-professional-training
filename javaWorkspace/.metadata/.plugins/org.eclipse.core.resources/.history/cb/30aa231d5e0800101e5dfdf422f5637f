package com.am;

import java.awt.Button;
import java.awt.Frame;
import java.awt.Panel;

public class Ex06 extends Frame {

	public Ex06() {
		setLayout(null);
		Panel p = new Panel();
		Button btn1 = new Button("btn1");
		btn1.setLocation(100, 100);
		btn1.setSize(100, 50);
		add(btn1);

		setLocation(100, 100);
		setSize(400, 300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new Ex06();
	}

}
