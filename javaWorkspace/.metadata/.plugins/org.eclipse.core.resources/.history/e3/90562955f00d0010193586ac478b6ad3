package com.step01;

import java.io.IOException;
import java.net.ServerSocket;

public class Server {

	public static void main(String[] args) {
		java.net.ServerSocket serv;

		try {
			serv = new ServerSocket(3000);
			
			
			
			serv.close();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}

}
