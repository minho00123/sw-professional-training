package com.pm;
// 주민번호체크
// 학생성적
class Student {
	int kor;
	int eng;
	int math;
	public void setKor(String kor) {
		this.kor=Integer.parseInt(kor);
	}
	public void setEng(String eng) {
		this.eng=Integer.parseInt(eng);
	}
	public void setMath(String math) {
		this.math=Integer.parseInt(math)
	}
}

