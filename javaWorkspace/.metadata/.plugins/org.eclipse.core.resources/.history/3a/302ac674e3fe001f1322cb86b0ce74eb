package com.am;

class Car {
	public String model,color;
	public int speed;
	public final int max;
	
	public Car(String color, String model, int max) {
		this.color=color;
		this.model=model;
		this.max=max;
	}
	
	public void up() {
		if(speed+10>max)
			speed=max;
		else speed+=10;
	}
}

public class Ex02 {

	public static void main(String[] args) {
		

	}

}
