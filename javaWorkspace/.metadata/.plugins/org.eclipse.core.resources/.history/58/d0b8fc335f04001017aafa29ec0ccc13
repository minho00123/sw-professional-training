package com.am;

class Ball{
	static int cnt=1;
	final int num;
	boolean boo;
	
	public Ball() {
		num=cnt++;
		boo=false;
	}
	public void check() {
		boo=true;
	}
}

public class Ex01 {

	public static void main(String[] args) {
		Ball[] box=new Ball[45];
		
		for(int i=0; i<45; i++) {
			box[i]=new Ball();
		}
	}

}
