package com.am;

class Outter03{
	static int su1=1111;
	int su2=2222;
	
	class Inner03{
		final static int su3=3333;
//		static int su3=3333; // 오류!
//		static void func03() {} // 오류!
		int su4=4444;
		void func04() {}
	}
	
	static void func01() {
		System.out.println(Inner03.su3);
	}
	void func02() {}
}

public class Ex03 {

	public static void main(String[] args) {

	}

}
