package com.pm;

import java.awt.Frame;
import java.awt.GridLayout;

public class Ex12 extends Frame {

	public Ex12() {
		setLayout(new GridLayout(6, 9));
		for (int i = 0; i < 6; i++) {
			add(new Label());
		}
		setLocation(100, 100);
		setSize(500, 300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new Ex12();
	}

}
