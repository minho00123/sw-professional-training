package com.am;

import java.util.Optional;

public class Ex11 {

	public static String func01() {
		return null;
	}

	public static void main(String[] args) {
		// Optional
		String msg = Ex11.func01();
		if (msg != null) 
			msg = msg.trim();
		System.out.println(msg);
		Optional op = new Optional();
	}

}
