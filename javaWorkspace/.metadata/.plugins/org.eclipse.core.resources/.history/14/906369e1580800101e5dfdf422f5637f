package com.am;

import java.awt.Button;
import java.awt.Frame;
import java.awt.GridBagLayout;

public class Ex05 extends Frame {

	public Ex05() {
		
		GridBagLayout mgr = new GridBagLayout();
		setLayout(mgr);
		
		Button btn1 = new Button();
		
		setLocation(100, 100);
		setSize(400, 300);
		setVisible(true);
	}

	public static void main(String[] args) {
		new Ex05();
	}

}
