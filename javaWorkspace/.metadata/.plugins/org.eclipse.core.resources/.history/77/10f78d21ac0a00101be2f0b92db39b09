package com.am;

import java.awt.Frame;

public class Ex01 extends Frame {

	public Ex01() {
		
		setBounds(100, 100, 500, 300);
		setVisible(true);
		System.out.println(Thread.currentThread().getName());
	}

	public static void main(String[] args) {
		// ¾²·¹µå
		new Ex01();
		System.out.println(Thread.currentThread().getName());
	}

}
