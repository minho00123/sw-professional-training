package com.am;

class Outter04{
	static int su3=3333;
	int su4=4444;
	static void func03() {}
	void func04() {}
	
	static void func01() {
		class Local01{ // 접근제한자 불가능
			public final static int su01=1111; // 그냥 static은 불가능
			public int su02=2222;
			public void func01() {
				System.out.println(su01);
				System.out.println(su02);
				func02();
				func03();
				func04();
			}
			public void func02() {}
		}
		Local01 loc=new Local01();
		System.out.println(Local01.su01);
		System.out.println(loc.su02);
		loc.func01();
	}
//	new Local01(); 불가능
	void func02() {
		class Local02{}// 위와 동일
	}
}

public class Ex04 {

	public static void main(String[] args) {

	}

}
