package com.am;

interface Parent{
	void func02();
}
class Lec05{
// 클로져
	static Parent func01() {
		class Local implements Parent{
			public void func02() {
				System.out.println("내부클래스의 기능수행");
			}
		}
//		local.func02();
		return new Local();
	}
}

public class Ex05 {

	public static void main(String[] args) {
		// 익명클래스 <- 엄청 많이 사용됨
		Parent local=Lec05.func01();
		local.func02();

	}

}
