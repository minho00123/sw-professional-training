package com.am;

import java.util.*; // 유틸리티 도구

public class Ex01 {

	public static void main(String[] args) {
		int[] arr1 = { 1, 3, 5, 7, 9 };

		String msg1 = Arrays.toString(arr1);
		System.out.println(arr1.toString());
		System.out.println(msg1);
		
		int[] arr2 = Arrays.copyOf(arr1, 5);
		
		System.out.println(arr2);
	}

}
