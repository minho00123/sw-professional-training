package com.am;

interface Parent{
	void func02();
}
class Lec05{
// 클로저
	static Parent func01() {
//		local.func02();
		Parent local=new Parent(){
			public void func02() {
				System.out.println("내부클래스의 기능수행");
			}
		};
		return local;
	}
}

public class Ex05 {

	public static void main(String[] args) {
		// 익명클래스 <- 엄청 많이 사용됨
		Parent local=Lec05.func01();
		local.func02();

	}

}
