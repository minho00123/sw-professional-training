package com.am;

public class Ex05 extends Thread {

	public static void main(String[] args) {

	}
	
	@Override
	public void run() {
		System.out.println(getName() + " start");
		try {
			Thread.sleep(10000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		System.out.println(getName() + " end");
	}

}
